{
    "name": "api-payments-history",
    "version": "1.0.0",
    "description": "For an interview process, a payments history API",
    "main": "server.js",
    "scripts": {
        "start": "node -r dotenv/config server.js",
        "dev": "nodemon -r dotenv/config server.js",
        "test": "dotenv jest",
        "test:unit": "jest __tests__/unit",
        "test:integration": "dotenv jest __tests__/integration",
        "test:watch": "dotenv jest --watch",
        "test:coverage": "dotenv jest --coverage",
        "test:coverage:open": "dotenv jest --coverage && open coverage/lcov-report/index.html",
        "cdk": "dotenv cdk",
        "deploy": "dotenv cdk deploy",
        "synth": "dotenv cdk synth",
        "bootstrap": "dotenv cdk bootstrap"
    },
    "keywords": [
        "payments",
        "api",
        "express",
        "nodejs"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "serverless-http": "^3.2.0"
    },
    "devDependencies": {
        "@aws-sdk/client-dynamodb": "^3.400.0",
        "@aws-sdk/lib-dynamodb": "^3.400.0",
        "@eslint/js": "^9.36.0",
        "aws-cdk-lib": "^2.100.0",
        "constructs": "^10.3.0",
        "dotenv-cli": "^7.3.0",
        "eslint": "^9.36.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "supertest": "^6.3.3"
    },
    "engines": {
        "node": "22"
    }
}